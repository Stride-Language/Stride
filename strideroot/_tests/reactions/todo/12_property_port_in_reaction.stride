use DesktopAudio version 1.0

module Reaction {
    ports: [
        mainOutputPort OutputPort {
            block: Output
        }
        mainInputPort InputPort {
            block: Input
        }
        propertyInputPort PropInputPort {
			name: "test"
            block: Test
        }
    ]
	blocks: [
        reaction PassThruReaction {
            streams:    [
                AudioIn[0] * Test >> AudioOut[0];
            ]
        }
    ]
    streams: [
        on >> PassThruReaction();
    ]
}

signal UsedInReaction { default: 0.3}

on >> Reaction (test:UsedInReaction) >> AudioOut;