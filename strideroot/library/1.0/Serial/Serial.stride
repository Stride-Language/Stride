
module SerialPrint {
    ports: [
        mainInputPort ValuePort {
            name:       'value'
			block: Value
        }
        propertyInputPort ChannelPort {
            name:       'value'
			block: Channel
        }
	]
    blocks: [
		signal Value {
			default: 0
			domain: ValuePort.domain
		}
		signal Channel {
			default: 0
			domain: ValuePort.domain
		}		
    ]
    streams: [
        [Value, Channel] >> _SerialPrintModule();
    ]
}

