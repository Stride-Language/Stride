use DesktopAudio version 1.0

import Generators

table TestTable {
	size: 1024
	rate: 1024
	domain: SetupDomain
}

trigger CounterReset {
	domain: AudioDomain
}

signal Counter {
	default: 0.0
	domain: AudioDomain
	reset: CounterReset
}

[0.5, 1.5] >> SineTable; 

[Counter, 1024] >> Equal() >> CounterReset;

SineTable[Counter] >> AudioOut[0];
Counter + 1 >> Counter;

SineTable[Counter] >> AudioOut[1];
Counter + 1 >> Counter;
