use DesktopAudio version 1.0

import Generators

table SineTable {
	size: 1024
	rate: 1024
	domain: SetupDomain
	reset: none
}

trigger CounterReset {
	domain: AudioDomain
}

signal Counter {
	default: 0.0
	domain: AudioDomain
	reset: CounterReset
}

Oscillator(frequency: 1.0) >> SineTable; 

Counter + 1 >> Counter;

[Counter, 1024] >> Equal() >> CounterReset;

SineTable[Counter] >> AudioOut[0];