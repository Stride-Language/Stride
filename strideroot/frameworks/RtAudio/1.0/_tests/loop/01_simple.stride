use DesktopAudio version 1.0

signal Accum {}

loop Loop {
	ports: [		
	]
	blocks: [
		signal Counter {}
		switch CounterReset {}
	]
	streams: [
		Accum + 2 >> Accum;
		Counter + 1 >> Counter;
		[Counter, 10] >> Equal() >> CounterReset;
	]
	terminateWhen: CounterReset
}


on >> Loop();
Accum >> AudioOut[1];

